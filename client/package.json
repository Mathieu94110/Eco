    "axios": "^1.1.3",
    "core-js": "^3.26.1",
    "es6-promise": "^4.2.8",
    "mitt": "^3.0.0",
    "mosha-vue-toastify": "^1.0.23",
    "register-service-worker": "^1.7.2",
    "swiper": "^10.0.0",
    "vee-validate": "^4.11.8",
    "vue": "^3.2.41",
    "vue-loading-overlay": "^6.0.2",
